<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="scroll-pane" stylesheets="@style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.Ankiety_PZ.panele.LadowanieBazyController">
   <children>
      <Label fx:id="info" layoutX="2.0" layoutY="186.0" prefHeight="33.0" prefWidth="594.0">
         <font>
            <Font size="8.0" />
         </font></Label>
      <Label fx:id="error" layoutX="14.0" layoutY="362.0" prefHeight="33.0" prefWidth="584.0" styleClass="error" stylesheets="@style.css" textFill="RED">
         <font>
            <Font size="8.0" />
         </font></Label>
      <Label fx:id="linkLabel" layoutX="16.0" layoutY="16.0" prefHeight="33.0" prefWidth="263.0" text="Adres serwera mySQL:" />
      <Label fx:id="loginLabel" layoutX="16.0" layoutY="116.0" prefHeight="33.0" prefWidth="263.0" text="Nazwa użytkownika:" />
      <Label fx:id="hasloLabel" layoutX="16.0" layoutY="216.0" prefHeight="33.0" prefWidth="263.0" text="Hasło:" />
      <TextField fx:id="login" layoutX="16.0" layoutY="156.0" prefHeight="47.0" prefWidth="561.0" />
      <TextField fx:id="haslo" layoutX="16.0" layoutY="256.0" prefHeight="47.0" prefWidth="561.0" />
      <Button fx:id="dalej" layoutX="452.0" layoutY="312.0" mnemonicParsing="false" onAction="#dalejButtonClick" prefHeight="50.0" prefWidth="125.0" text="Dalej" />
      <TextField fx:id="link" layoutX="16.0" layoutY="56.0" prefHeight="47.0" prefWidth="561.0" />
       <RadioButton fx:id="yes" layoutX="306.0" layoutY="322.0" mnemonicParsing="false" selected="true" text="Z danymi">
           <toggleGroup>
               <ToggleGroup fx:id="radioButtonsGroup" />
           </toggleGroup>
       </RadioButton>
       <RadioButton fx:id="no" layoutX="112.0" layoutY="322.0" mnemonicParsing="false" text="Bez danych (niezalecane)" toggleGroup="$radioButtonsGroup" />
   </children>
</AnchorPane>
